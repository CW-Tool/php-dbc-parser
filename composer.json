{
    "name": "wowstack/dbc-parser",
    "description": "Read and write WoW DBC files",
    "version": "0.1.0",
    "type": "library",
    "homepage": "https://wowstack.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel S. Reichenbach",
            "email": "daniel@kogitoapp.com"
        }
    ],
    "require": {
        "symfony/console": "^4.0",
        "symfony/yaml": "^4.0",
        "doctrine/inflector": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Wowstack\\Dbc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wowstack\\Dbc\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
