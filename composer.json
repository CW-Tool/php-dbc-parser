{
    "name": "wowstack/dbc-parser",
    "description": "Read World of Warcraft DBC files",
    "version": "0.1.0",
    "type": "library",
    "homepage": "https://wowstack.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel S. Reichenbach",
            "email": "daniel@kogitoapp.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/console": "^4.0",
        "symfony/yaml": "^4.0",
        "doctrine/inflector": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "egosun/symfony-4-code-sniffer": "^0.0.3"
    },
    "autoload": {
        "psr-4": {
            "Wowstack\\Dbc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wowstack\\Dbc\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "bin": [
        "bin/dbc-tool"
    ]
}
